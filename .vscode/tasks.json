{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "process",
			"label": "Run Makefile",
			
			"command": "/usr/bin/make",
			"group": "build",
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"type": "process",
			"label": "Clean",
			
			"command": "/usr/bin/make",
			"args": [
				"clean"
			],
			"group": "build",
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}